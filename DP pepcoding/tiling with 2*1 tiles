import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc =new Scanner (System.in);
		int n=sc.nextInt();
		int dp[]=new int[n+1];
		dp[1]=1;
		dp[2]=2;
		for(int i=3;i<=n;i++){
		    dp[i]=dp[i-1]+dp[i-2];
		}
		System.out.println(dp[n]);
	}
}
